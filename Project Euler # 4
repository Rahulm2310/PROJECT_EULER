using namespace std;
int ispallindrome(int);

int main(){
    int t;
    cin >> t;
    for(int a0 = 0; a0 < t; a0++){
        int n;
        cin >> n;
        int maxpal=101101,pal;
        for(int i=100;i<999;i++)
        {
            for(int j=i+1;j<=999;j++)
            {
                if((i*j)<n)
                {
                    pal=i*j;
                    if(ispallindrome(pal))
                    {
                        if(maxpal<pal)
                        {
                            maxpal=pal;
                        }
                    }
                }
            }
        }
        cout<<maxpal<<endl;
    }
return 0;
 }

int ispallindrome(int n)
{
     int t,reverse=0;
       t = n;
 
   while (t != 0)
   {
      reverse = reverse * 10;
      reverse = reverse + t%10;
      t = t/10;
   }
 
   if (n == reverse)
      return 1;
   else
      return 0;
}
